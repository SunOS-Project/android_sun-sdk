    <!-- Pocket Lock -->
    <bool name="config_pocketModeSupported">true</bool>
    <bool name="config_pocketUseLightSensor">true</bool>

    <!-- Defines custom OEM sensor for pocket detection. -->
    <string name="config_pocketJudgeVendorSensorName"></string>
    <string name="config_pocketJudgeVendorProximitySensorName"></string>

    <!-- Custom doze -->
    <string name="config_doze_pickUpSensorType"></string>
    <item name="config_doze_pickUpSensorValue" format="float" type="dimen">0.0</item>

    <!-- Pop-Up View -->
    <bool name="config_popUpView_defaultKeepMuteInMini">true</bool>
    <integer name="config_popUpView_defaultSingleTapAction">1</integer>
    <integer name="config_popUpView_defaultDoubleTapAction">2</integer>
    <bool name="config_popUpView_defaultNotificationPortJump">false</bool>
    <bool name="config_popUpView_defaultNotificationLandJump">false</bool>
    <bool name="config_popUpView_defaultSettingsJump">false</bool>
    <bool name="config_popUpView_defaultShareJump">false</bool>
    <string-array name="config_popUpView_forceUpdateDpiPackages" translatable="false">
    </string-array>

    <!-- Whether to enable lockscreen charging info settings -->
    <bool name="config_enable_lockscreen_charging_info">true</bool>

    <!-- Whether allow to show charging remaining time -->
    <bool name="config_show_charging_remaining_time">true</bool>

    <!-- Launchers that supported in launcher switcher -->
    <string-array name="config_launcherSwitcherComponents" translatable="false">
        <item>com.google.android.apps.nexuslauncher/com.android.quickstep.RecentsActivity</item>
        <item>com.android.launcher3/com.android.quickstep.RecentsActivity</item>
    </string-array>

