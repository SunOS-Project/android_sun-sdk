<resources>

    <!-- Name of a font family to use as light font. For theming purpose. -->
    <string name="config_lightFontFamily" translatable="false">sans-serif-light</string>

    <!-- Name of a font family to use as regular font. For theming purpose. -->
    <string name="config_regularFontFamily" translatable="false">sans-serif-regular</string>

    <!-- Whether device has physical tri-state switch -->
    <bool name="config_hasAlertSlider">false</bool>

    <!-- The location of the devices physical tri state switch
         0: Left side
         1: Right side -->
    <integer name="config_alertSliderLocation">0</integer>

    <!-- Overlays that to be enabled in FHD+ resolution -->
    <string-array name="config_fhdResolutionOverlays" translatable="false">
    </string-array>

    <!-- Expose aux camera to other packages -->
    <string name="oplus_camera_aux_packages"></string>
    <string name="oplus_camera_aux_packages_startwith"></string>

    <!-- Define device configs on boot -->
    <string-array name="global_device_configs_override" translatable="false">
    </string-array>
    <string-array name="device_configs_override" translatable="false">
    </string-array>

    <!-- List of system apps that disallowed from being cloned -->
    <string-array name="config_nonCloneableSystemAppList" translatable="false">
        <item>com.android.cellbroadcastreceiver.module</item>
        <item>com.android.launcher3</item>
        <item>com.android.music</item>
        <item>com.android.stk</item>
        <item>com.dolby.daxappui</item>
        <item>com.google.android.accessibility.soundamplifier</item>
        <item>com.google.android.apps.aiwallpapers</item>
        <item>com.google.android.apps.emojiwallpaper</item>
        <item>com.google.android.apps.messaging</item>
        <item>com.google.android.apps.nexuslauncher</item>
        <item>com.google.android.apps.restore</item>
        <item>com.google.android.apps.wallpaper</item>
        <item>com.google.android.apps.weather</item>
        <item>com.google.android.apps.wellbeing</item>
        <item>com.google.android.apps.work.clouddpc</item>
        <item>com.google.android.contacts</item>
        <item>com.google.android.deskclock</item>
        <item>com.google.android.dialer</item>
        <item>com.google.android.euicc</item>
        <item>com.google.android.pixel.setupwizard</item>
        <item>com.google.android.projection.gearhead</item>
        <item>com.google.android.setupwizard</item>
        <item>com.google.android.wallpaper.effects</item>
        <item>com.google.audio.hearing.visualization.accessibility.scribe</item>
        <item>com.google.pixel.livewallpaper</item>
        <item>com.motorola.dolby.dolbyui</item>
        <item>com.oneplus.camera</item>
        <item>com.oneplus.gallery</item>
        <item>com.oplus.camera</item>
        <item>org.lineageos.aperture</item>
    </string-array>
    <!-- List of user apps that disallow from being cloned -->
    <string-array name="config_nonCloneableUserAppList" translatable="false">
    </string-array>

    <!-- Pocket Lock -->
    <bool name="config_pocketModeSupported">true</bool>
    <bool name="config_pocketUseLightSensor">true</bool>

    <!-- Defines custom OEM sensor for pocket detection. -->
    <string name="config_pocketJudgeVendorSensorName"></string>
    <string name="config_pocketJudgeVendorProximitySensorName"></string>

    <!-- Custom doze -->
    <string name="config_doze_pickUpSensorType"></string>
    <item name="config_doze_pickUpSensorValue" format="float" type="dimen">0.0</item>

    <!-- Pop-Up View -->
    <bool name="config_popUpView_defaultGestureEnabled">true</bool>
    <bool name="config_popUpView_defaultShowMoreCircles">false</bool>
    <bool name="config_popUpView_defaultKeepMuteInMini">true</bool>
    <integer name="config_popUpView_defaultSingleTapAction">1</integer>
    <integer name="config_popUpView_defaultDoubleTapAction">2</integer>
    <bool name="config_popUpView_defaultNotificationPortJump">false</bool>
    <bool name="config_popUpView_defaultNotificationLandJump">false</bool>
    <string-array name="config_popUpView_forceUpdateDpiPackages" translatable="false">
    </string-array>

    <!-- Whether to enable lockscreen charging info settings -->
    <bool name="config_enable_lockscreen_charging_info">true</bool>

    <!-- Whether allow to show charging remaining time -->
    <bool name="config_show_charging_remaining_time">true</bool>

    <!-- Launchers that supported in launcher switcher -->
    <string-array name="config_launcherSwitcherComponents" translatable="false">
        <item>com.google.android.apps.nexuslauncher/com.android.quickstep.RecentsActivity</item>
        <item>com.android.launcher3/com.android.quickstep.RecentsActivity</item>
    </string-array>

    <!-- Default volume panel position in portrait mode
         0: Left side (default)
         1: Right side -->
    <integer name="config_volumePanelPosPortDefault">0</integer>

    <!-- Default volume panel position in landscape mode
         0: Left side
         1: Right side (default) -->
    <integer name="config_volumePanelPosLandDefault">1</integer>

    <!-- Whether device has a centered cutout -->
    <bool name="config_hasCenteredCutout">false</bool>

    <!-- Should we listen for fingerprints when the screen is off?  Devices
         with a rear-mounted sensor want this, but certain devices have
         the sensor embedded in the power key and listening all the time
         causes a poor experience. -->
    <bool name="config_fingerprintWakeAndUnlock">true</bool>

</resources>
